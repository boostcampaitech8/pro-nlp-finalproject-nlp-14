# 테스트 환경 설정
APP_ENV=test
DEBUG=true

# 테스트 데이터베이스 (별도 DB 사용)
DATABASE_URL=postgresql+asyncpg://mit:mitpassword@localhost:5432/mit_test

# Redis (테스트용 DB 1 사용)
REDIS_URL=redis://localhost:6379/1

# JWT (테스트용 키)
